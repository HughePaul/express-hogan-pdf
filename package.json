    "xml-to-pdf": "^1.0.4"
  },
  "peerDependencies": {
    "hogan.js": ">=3"
  },
  "devDependencies": {
    "hogan.js": "^3.0.2",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "istanbul": "^0.4.3",
